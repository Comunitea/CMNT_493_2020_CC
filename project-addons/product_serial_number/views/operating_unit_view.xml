<?xml version="1.0" encoding="utf-8" ?>
<odoo>
        <!-- operating.unit inherit form view -->
        <record id="cc_view_operating_unit_form" model="ir.ui.view">
            <field name="name">cc_view_operating_unit_form</field>
            <field name="model">operating.unit</field>
            <field name="inherit_id" ref="operating_unit.view_operating_unit_form" />
            <field name="arch" type="xml">
                <field name="code" position="after">
                    <field name="lot_seq" />
                    <field name="store_code"/>
                </field>
            </field>
        </record>
</odoo>
